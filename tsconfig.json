{
    "compilerOptions": {
      "outDir": "./dist",
      "rootDir": ".",
      "target": "ES2020",
      "module": "ES2020",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }
  